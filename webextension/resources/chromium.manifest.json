{
    "manifest_version": 2,
    "name": "Tab search provider for GNOME",
    "description": "Tab search provider for GNOME Shell",
    "version": "1.1.0",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "tabs",
        "nativeMessaging"
    ]
}