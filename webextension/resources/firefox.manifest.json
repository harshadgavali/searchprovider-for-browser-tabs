{
    "manifest_version": 2,
    "name": "Chromium Tab SearchProvider",
    "description": "Chromium Tab SearchProvider",
    "version": "1.0",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "tabs",
        "nativeMessaging"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "searchproviderconnector@searchproviderconnector"
        }
    }
}